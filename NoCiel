#include<iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
   int t;
   
   cin>>t;
   for(int i=0;i<t;i++)
   {
      double l,r,m,temp;
      int ans;
      cin>>l>>r>>m;
      temp=m/l;
      if(int(temp)!=temp)
      {
         temp++;
      }
      else{
         temp=temp;
      }
ans=int(temp)+int(m/r);
      cout<<(ans)<<endl;
      }
   return 0;
   }
   



